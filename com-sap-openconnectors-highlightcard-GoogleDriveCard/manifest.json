{
    "_version": "1.14.0",
    "sap.app": {
        "id": "com.sap.openconnectors.highlightcard.GoogleDriveCard",
        "type": "card",
        "title": "Google Drive Files",
        "subTitle": "",
        "applicationVersion": {
            "version": "1.0.0"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "phone": true,
            "tablet": true
        },
        "icons": {
            "icon": "sap-icon://list"
        }
    },
    "sap.card": {
        "type": "List",
        "designtime": "dt/configuration",
        "header": {
            "title": "Google Drive",
            "subTitle": "Files",
            "icon": {
                "src": "sap-icon://fallback"
            },
            "status": {
                "text": ""
            }
        },
        "content": {
            "item": {
                "title": "{name}",
                "description": "{properties/mimeType}",
                "icon": {
                    "src": "{properties/thumbnailLink}"
                }
            },
            "data": {
                "request": {
                    "url": "https://api.openconnectors.trial.eu10.ext.hana.ondemand.com/elements/api-v2/folders/contents?path=%2FSAP",
                    "headers": {
                        "accept": "application/json",
                        "Authorization": "User <<YOUR AUTHORIZATION DETAILS>>, Organization <<YOUR ORGANIZATION DETALS>>, Element <<YOUR ELEMENTS DETAILS>>"
                    }
                }
            }
        }
    },
    "sap.platform.mobilecards": {
        "compatible": false
    }
}